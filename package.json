{
  "name": "cibulb",
  "version": "0.1.0",
  "private": true,
  "repository": "git@github.com:screendriver/cibulb.git",
  "author": "Christian Rackerseder <git@echooff.de>",
  "license": "MIT",
  "scripts": {
    "lint": "tslint -c tslint.json 'src/**/*.ts' 'test/**/*.ts'",
    "test:unit": "nyc tape -r source-map-support/register 'dist/test/unit/**/*.js'",
    "test": "yarn lint && yarn test:unit | tap-dot",
    "test:ci": "yarn lint && yarn test:unit | tap-xunit > TEST-RESULTS.xml",
    "coverage": "nyc report --reporter=text-summary",
    "build": "tsc --build"
  },
  "dependencies": {
    "tslib": "1.9.3"
  },
  "devDependencies": {
    "@types/sinon": "7.0.3",
    "@types/tape": "4.2.33",
    "husky": "1.2.1",
    "nyc": "13.1.0",
    "prettier": "1.15.3",
    "pretty-quick": "1.8.0",
    "sinon": "7.2.2",
    "source-map-support": "0.5.9",
    "tap-dot": "2.0.0",
    "tap-xunit": "2.3.0",
    "tape": "4.9.2",
    "tslint": "5.11.0",
    "tslint-config-prettier": "1.17.0",
    "typescript": "3.2.2",
    "typescript-tslint-plugin": "0.2.0"
  }
}
