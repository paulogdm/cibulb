{
  "name": "cibulb",
  "version": "0.1.0",
  "private": true,
  "repository": "git@github.com:screendriver/cibulb.git",
  "author": "Christian Rackerseder <git@echooff.de>",
  "license": "MIT",
  "engines": {
    "node": "10.x"
  },
  "scripts": {
    "build": "tsc --build",
    "lint": "tslint -c tslint.json 'src/**/*.ts' 'test/**/*.ts'",
    "prettier:check": "prettier --check 'src/**/*.ts?(x)' 'test/**/*.ts?(x)'",
    "test:ci": "yarn lint && yarn prettier:check && yarn test:unit | tap-xunit > test-results.xml && yarn test:integration",
    "test:integration": "tape -r source-map-support/register 'target/test/integration/**/*.js' | tap-dot",
    "test:unit": "nyc tape -r source-map-support/register 'target/test/unit/**/*.js'",
    "test": "yarn lint && yarn prettier:check && yarn test:unit | tap-dot && yarn test:integration"
  },
  "dependencies": {
    "@sentry/node": "5.4.3",
    "got": "9.6.0",
    "loglevel": "1.6.3",
    "micro": "9.3.4",
    "mongodb": "3.1.13",
    "tslib": "1.10.0"
  },
  "devDependencies": {
    "@types/got": "9.6.0",
    "@types/loglevel": "1.5.4",
    "@types/micro": "7.3.3",
    "@types/mongodb": "3.1.22",
    "@types/mongodb-memory-server": "1.8.0",
    "@types/node": "8.10.49",
    "@types/sinon": "7.0.13",
    "@types/tape": "4.2.33",
    "@types/test-listen": "1.1.0",
    "mongodb-memory-server": "5.1.4",
    "now": "15.6.0",
    "nyc": "14.1.1",
    "prettier": "1.18.2",
    "sinon": "7.3.2",
    "source-map-support": "0.5.12",
    "tap-dot": "2.0.0",
    "tap-xunit": "2.4.1",
    "tape": "4.10.2",
    "test-listen": "1.1.0",
    "tslint": "5.18.0",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.5.2",
    "typescript-tslint-plugin": "0.5.2"
  }
}
