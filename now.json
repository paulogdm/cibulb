{
  "version": 2,
  "name": "cibulb",
  "alias": "cibulb.now.sh",
  "regions": ["bru"],
  "routes": [
    {
      "src": "/api/color",
      "methods": ["POST"],
      "dest": "/api/color"
    },
    {
      "src": "/api/refresh",
      "methods": ["GET"],
      "dest": "/api/refresh"
    },
    {
      "src": "/(.*)",
      "status": 404
    }
  ]
}
